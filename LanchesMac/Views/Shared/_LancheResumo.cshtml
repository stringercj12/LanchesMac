@model Lanche

<div class="col-sm-3 col-md-3 col-lg-3">
    <div class="card" style="width: 18rem">
        <div class="card-body">
            <img src="@Model.ImagemUrl" class="card-img-top" />
            <h4>Preço: @Model.Preco.ToString("c")</h4>
            <h5 class="card-title">@Model.Nome</h5>
            <p>
                @Model.DescricaoCurta
            </p>
        </div>
    </div>
</div>